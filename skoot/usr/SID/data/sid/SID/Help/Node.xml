<object program="/usr/SID/obj/sidnode">
  <SID:Element ns="Help" tag="Node" recpoint="false">
    <SID:Children>
      <SID:Child node="OBJ(SID:Log:Description)"/>
      <SID:Child node="OBJ(SID:Help:Keywords)"/>
    </SID:Children>
    <SID:Attributes>
      <SID:Attribute id="title" atype="lpc_str" areadonly="false" aquery="query_title"/>
    </SID:Attributes>
    <SID:Iterator/>
    <SID:Callbacks>
      <SID:Callback call="set_title(title)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
