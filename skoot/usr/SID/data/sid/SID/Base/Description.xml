<object program="/usr/SID/obj/sidnode">
  <SID:Element type="xml_pcdata" ns="Base" tag="Description" recpoint="false" query="query_description(id,type)" delitem="remove_description(id,type)">
    <SID:Children/>
    <SID:Attributes>
      <SID:Attribute id="type" atype="xml_pcdata" areadonly="false"/>
    </SID:Attributes>
    <SID:Iterator var="type" call="query_description_types(id)"/>
    <SID:Callbacks>
      <SID:Callback call="set_description(id,type,CONTENT)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
