<object program="/usr/SID/obj/sidnode">
  <SID:Element ns="Base" tag="Bulk" recpoint="true" transient="false">
    <SID:Children>
      <SID:Child node="OBJ(SID:Base:Mass)"/>
      <SID:Child node="OBJ(SID:Base:Density)"/>
      <SID:Child node="OBJ(SID:Base:Capacity)"/>
      <SID:Child node="OBJ(SID:Base:MaxWeight)"/>
    </SID:Children>
    <SID:Attributes>
      <SID:Attribute id="flexible" atype="xml_bool" acomment="Is the container flexible? Flexible means that the container crunches up, like a bag, rather then is rigid like a box." areadonly="false" aquery="query_flexible"/>
    </SID:Attributes>
    <SID:Iterator/>
    <SID:Callbacks>
      <SID:Callback call="set_flexible(flexible)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
