    <SID:Children/>
    <SID:Attributes>
      <SID:Attribute id="property" atype="lpc_str" areadonly="false"/>
    </SID:Attributes>
    <SID:Iterator var="property" call="query_property_indices(#0)"/>
    <SID:Callbacks>
      <SID:Callback call="set_ascii_property(property,CONTENT,#1)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
