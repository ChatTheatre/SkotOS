<object program="/usr/SID/obj/sidnode">
  <SID:Element ns="Marrach" tag="SkillAlias" recpoint="false"
               delitem="remove_alias(alias)">
    <SID:Attributes>
      <SID:Attribute id="alias" atype="lpc_str" areadonly="false"/>
    </SID:Attributes>
    <SID:Iterator var="alias" call="list_aliases(skill)"/>
    <SID:Callbacks>
      <SID:Callback call="add_alias(skill, alias)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
