    <SID:Child node="OBJ(SID:TextIF:CombatEntry)"/>
  </SID:Children>
  <SID:Attributes>
    <SID:Attribute id="name" atype="lpc_str" aquery="query_name"/>
  </SID:Attributes>
  <SID:Iterator/>
