<object program="/usr/SID/obj/sidnode">
  <SID:Element ns="FileSystem" tag="File" recpoint="false">
    <SID:Children/>
    <SID:Attributes>
      <SID:Attribute id="file" atype="dev_fname" areadonly="false"/>
    </SID:Attributes>
    <SID:Iterator var="file" call="query_files(path)"/>
    <SID:Callbacks>
      <SID:Callback call="add_file(file)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
