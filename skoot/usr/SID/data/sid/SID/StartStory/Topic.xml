<object program="/usr/SID/obj/sidnode">
  <SID:Element ns="StartStory" tag="Topic" recpoint="true">
    <SID:Children>
      <SID:Child node="OBJ(SID:StartStory:Description)"/>
    </SID:Children>
    <SID:Attributes>
      <SID:Attribute id="topic" atype="lpc_str" areadonly="false"/>
    </SID:Attributes>
    <SID:Iterator var="topic" call="query_topics"/>
    <SID:Callbacks>
      <SID:Callback call="clear_topic(topic)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
