<HTML>
  <HEAD>
    <TITLE>[$(System.hostname)] $(UDat.queryprop name="source-$(id)-title")</TITLE>
    <STYLE><!--
    body {
        background: $(tool.config:view-background);
        foreground: $(tool.config:view-foreground);
    }
    .comment {
        foreground: $(tool.config:view-comment);
    }
    .cpp {
        foreground: $(tool.config:view-cpp);
    }
    .error {
        foreground: $(tool.config:view-error);
    }
    .keyword {
        foreground: $(tool.config:view-keyword);
    }
    .linenr {
        foreground: $(tool.config:view-linenr);
    }
    .string {
        foreground: $(tool.config:view-string);
    }
    .type {
        foreground: $(tool.config:view-type);
    }
 --></STYLE>
  </HEAD>
  <BODY bgcolor="#ffffff">
    <TABLE border=1 cellpadding=5 cellspacing=0 width="100%">
      <TR><TH>$(UDat.queryprop name="source-$(id)-title")</TH></TR>
    </TABLE>
    <TABLE border=1 cellpadding=5 cellspacing=0 width="100%">
      <TR><TD><PRE>$(UDat.queryprop name="source-$(id)-text")</PRE></TD></TR></TABLE>
  </BODY>
</HTML>
