<HTML>
  <HEAD>
    <TITLE>$(Title)</TITLE>
  </HEAD>
  <BODY BGCOLOR=black CELLSPACING=0 CELLPADDING=0 BORDER=0 VSPACE=0
        MARGIN=0 TOPMARGIN=0 LEFTMARGIN=0 onload="DoResize();">
    <IMG ID="Art" name="Art" SRC="$(url)" BORDER=0 VSPACE=0
         MARGIN=0 TOPMARGIN=0 LEFTMARGIN=0 MARGINWIDTH=0 MARGINHEIGHT=0
         onload="DoResize();"/>
    <script language="javascript"><!--
	function DoResize() {
	    var winOffsetWidth = document.Art.width + 12;
	    var winOffsetHeight = document.Art.height + 31;
	    window.resizeTo(winOffsetWidth,winOffsetHeight);
	}
    //--></script>
  </BODY>
</HTML>
