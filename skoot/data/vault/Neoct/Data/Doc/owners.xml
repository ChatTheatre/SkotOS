         (\{ 1135371998, "-", "SYNC" \})
      </Core:Property>
      <Core:Property property="see_also">"+whereis"</Core:Property>
      <Core:Property property="synopsis">
         "+owners \\"woename\\""
      </Core:Property>
    </Core:PCProperties>
    <Notes:Notes/>
  </Core:PropertyContainer>
</object>
