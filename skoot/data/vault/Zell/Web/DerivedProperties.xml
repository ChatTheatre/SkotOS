<object clone="/obj/properties" owner="zell">
  <Core:PropertyContainer>
    <Ur:UrObject/>
    <Core:PCProperties>
      <Core:Property property="html-extras:show-properties">
         X[S] \<h4\>Derived properties available on \$(handler):\</h4\>
\<table width="100%" border="1" cellspacing="0" cellpadding="0"\>
 \<for var="ix" val="\$(handler.derived:properties)"\>
  \<tr valign="top"\>
   \<td\>\{? \| \$[Get(\$handler, "derived:has-local:" + \$ix)] \| X \| \\\&nbsp;\}\</td\>
   \<td\>\$[Get(\$handler, "derived:accessdesc:" + \$ix)]\</td\>
   \<td\>\$(handler.derived:root):\$(ix)\</td\>
   \<td\>\$[Get(\$handler, "derived:typedesc:" + \$ix)]\</td\>
   \<td width="60%"\>\$[Get(\$handler, "derived:desc:" + \$ix)]\</td\>
  \</tr\>
 \</for\>
\</table\>
      </Core:Property>
      <Core:Property property="html:show-base-properties">
         X[S] \<html\>
 \<head\>
  \<title\>Derived Properties\</title\>
 \</head\>
 \<body bgcolor="#404040" text="yellow"\>
   \$(this.html-extras:show-properties handler="\$[\$\{Base:DerivedBase\}]")
 \</body\>
\</html\>
      </Core:Property>
      <Core:Property property="html:show-detail-properties">
         X[S] \<html\>
 \<head\>
  \<title\>Derived Properties\</title\>
 \</head\>
 \<body bgcolor="#404040" text="yellow"\>
   \$(this.html-extras:show-properties handler="\$[\$\{Base:DerivedDetails\}]")
 \</body\>
\</html\>
      </Core:Property>
      <Core:Property property="revisions">
         (\{ 1050529124, "zell", "E", 1050529239, "zell", "E", 1050529252, "zell", "E", 1050529805, "zell", "E", 1050559129, "zell", "E", 1050559201, "zell", "E", 1050559459, "zell", "E", 1050559507, "zell", "E", 1050560389, "zell", "E", 1050615393, "zell", "E" \})
      </Core:Property>
    </Core:PCProperties>
    <Notes:Notes/>
  </Core:PropertyContainer>
</object>
