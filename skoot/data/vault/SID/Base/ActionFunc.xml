<object clone="/usr/SID/obj/sidnode" owner="SID">
  <SID:Element ns="Base" tag="ActionFunc" recpoint="false" transient="false">
    <SID:Children>
      <SID:Child node="OBJ(SID:Base:FuncArgs)"/>
    </SID:Children>
    <SID:Attributes>
      <SID:Attribute id="func" atype="lpc_str" areadonly="false"/>
    </SID:Attributes>
    <SID:Iterator var="func" call="query_action_funcs(action)"/>
    <SID:Callbacks>
      <SID:Callback call="add_action_func(action, func)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
