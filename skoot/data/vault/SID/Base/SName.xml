<object clone="/usr/SID/obj/sidnode" owner="nino">
  <SID:Element ns="Base" tag="SName" recpoint="false" transient="false" delitem="remove_sname(id, sname)">
    <SID:Children/>
    <SID:Attributes>
      <SID:Attribute id="sname" atype="lpc_str" areadonly="false"/>
    </SID:Attributes>
    <SID:Iterator var="sname" call="query_snames(id)"/>
    <SID:Callbacks>
      <SID:Callback call="add_sname(id, sname)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
