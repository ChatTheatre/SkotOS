      <SID:Child node="OBJ(SID:Base:PName)"/>
    </SID:Children>
    <SID:Attributes/>
    <SID:Iterator/>
    <SID:Callbacks>
      <SID:Callback call="clear_pnames(id)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
