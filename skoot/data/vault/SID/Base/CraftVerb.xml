<object clone="/usr/SID/obj/sidnode" owner="SID">
  <SID:Element ns="Base" tag="CraftVerb" recpoint="false" transient="false" newitem="add_verb(#NEW)">
    <SID:Children/>
    <SID:Attributes>
      <SID:Attribute id="verb" atype="lpc_str" areadonly="false"/>
    </SID:Attributes>
    <SID:Iterator var="verb" call="query_verbs"/>
    <SID:Callbacks>
      <SID:Callback call="add_verb(verb)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
