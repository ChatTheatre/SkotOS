<object clone="/usr/SID/obj/sidnode" owner="SID">
  <SID:Element ns="Base" tag="Adjective" recpoint="false" transient="false" delitem="remove_adjective(id, adjective)">
    <SID:Children/>
    <SID:Attributes>
      <SID:Attribute id="adjective" atype="lpc_str" areadonly="false"/>
    </SID:Attributes>
    <SID:Iterator var="adjective" call="query_adjectives(id)"/>
    <SID:Callbacks>
      <SID:Callback call="add_adjective(id, adjective)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
