<object clone="/usr/SID/obj/sidnode" owner="SID">
  <SID:Element ns="Base" tag="Preposition" recpoint="false" transient="false">
    <SID:Children/>
    <SID:Attributes>
      <SID:Attribute id="close" atype="xml_bool" acomment="close to, close by, close beside" areadonly="false" aquery="test_id_prep(id, #1)"/>
      <SID:Attribute id="against" atype="xml_bool" acomment="against" areadonly="false" aquery="test_id_prep(id, #2)"/>
      <SID:Attribute id="under" atype="xml_bool" acomment="under, beneath, below" areadonly="false" aquery="test_id_prep(id, #4)"/>
      <SID:Attribute id="on" atype="xml_bool" acomment="on, upon, on top of, across, atop" areadonly="false" aquery="test_id_prep(id, #8)"/>
      <SID:Attribute id="inside" atype="xml_bool" acomment="inside, in, within" areadonly="false" aquery="test_id_prep(id, #16)"/>
      <SID:Attribute id="near" atype="xml_bool" acomment="near, by, nearby, at" areadonly="false" aquery="test_id_prep(id, #256)"/>
      <SID:Attribute id="over" atype="xml_bool" acomment="over, above" areadonly="false" aquery="test_id_prep(id, #512)"/>
      <SID:Attribute id="behind" atype="xml_bool" acomment="behind" areadonly="false" aquery="test_id_prep(id, #1024)"/>
      <SID:Attribute id="before" atype="xml_bool" acomment="before, in front of" areadonly="false" aquery="test_id_prep(id, #2048)"/>
      <SID:Attribute id="beside" atype="xml_bool" acomment="beside, next to" areadonly="false" aquery="test_id_prep(id, #4096)"/>
    </SID:Attributes>
    <SID:Iterator var="prep" call="query_id_preps(id)"/>
    <SID:Callbacks>
      <SID:Callback call="set_id_prep(id, #0001, close)"/>
      <SID:Callback call="set_id_prep(id, #0002, against)"/>
      <SID:Callback call="set_id_prep(id, #0004, under)"/>
      <SID:Callback call="set_id_prep(id, #0008, on)"/>
      <SID:Callback call="set_id_prep(id, #0016, inside)"/>
      <SID:Callback call="set_id_prep(id, #0256, near)"/>
      <SID:Callback call="set_id_prep(id, #0512, over)"/>
      <SID:Callback call="set_id_prep(id, #1024, behind)"/>
      <SID:Callback call="set_id_prep(id, #2048, before)"/>
      <SID:Callback call="set_id_prep(id, #4096, beside)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
