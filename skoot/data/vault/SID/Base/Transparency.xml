<object clone="/usr/SID/obj/sidnode" owner="SID">
  <SID:Element ns="Base" tag="Transparency" recpoint="false" transient="false">
    <SID:Children/>
    <SID:Attributes>
      <SID:Attribute id="value" atype="xml_bool" acomment="For clothing and other worn items, does it reveal items under it on the same location? I.E. rings, belts, etc." areadonly="false" aquery="query_transparency"/>
    </SID:Attributes>
    <SID:Iterator/>
    <SID:Callbacks>
      <SID:Callback call="set_transparency(value)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
