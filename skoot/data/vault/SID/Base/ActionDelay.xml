<object clone="/usr/SID/obj/sidnode" owner="SID">
  <SID:Element ns="Base" tag="ActionDelay" recpoint="false" transient="false">
    <SID:Children/>
    <SID:Attributes>
      <SID:Attribute id="delay" atype="lpc_int" areadonly="false"/>
    </SID:Attributes>
    <SID:Iterator var="delay" call="query_action_delays(action)"/>
    <SID:Callbacks>
      <SID:Callback call="add_action_delay(action, delay)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
