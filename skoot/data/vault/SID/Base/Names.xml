<object clone="/usr/SID/obj/sidnode" owner="SID">
  <SID:Element ns="Base" tag="Names" recpoint="false" transient="false" newitem="add_alias(id, #singular name, #plural name)">
    <SID:Children>
      <SID:Child node="OBJ(SID:Base:SNames)"/>
      <SID:Child node="OBJ(SID:Base:PNames)"/>
    </SID:Children>
    <SID:Attributes/>
    <SID:Iterator/>
    <SID:Callbacks>
      <SID:Callback call="clear_names(id)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
