<object clone="/usr/SID/obj/sidnode" owner="nino">
  <SID:Element ns="Base" tag="PName" recpoint="false" transient="false" delitem="remove_pname(id, pname)">
    <SID:Children/>
    <SID:Attributes>
      <SID:Attribute id="pname" atype="lpc_str" areadonly="false"/>
    </SID:Attributes>
    <SID:Iterator var="pname" call="query_pnames(id)"/>
    <SID:Callbacks>
      <SID:Callback call="add_pname(id, pname)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
