<object clone="/usr/SID/obj/sidnode" owner="SID">
  <SID:Element ns="Base" tag="ContentGroup" recpoint="false" transient="false" newitem="add_content_choice(group, #NEW)" delitem="clear_content_group(group)">
    <SID:Children>
      <SID:Child node="OBJ(SID:Base:ContentItem)"/>
    </SID:Children>
    <SID:Attributes>
      <SID:Attribute id="group" atype="lpc_str" areadonly="false"/>
    </SID:Attributes>
    <SID:Iterator var="group" call="query_content_groups"/>
    <SID:Callbacks>
      <SID:Callback call="clear_content_group(group)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
