<object clone="/usr/SID/obj/sidnode" owner="nino">
  <SID:Element ns="Tip" tag="Node" recpoint="false" transient="false">
    <SID:Children>
      <SID:Child node="OBJ(SID:Core:Properties)"/>
      <SID:Child node="OBJ(SID:Ur:UrObject)"/>
      <SID:Child node="OBJ(SID:Tip:Descriptions)"/>
    </SID:Children>
    <SID:Attributes>
      <SID:Attribute id="topic" atype="lpc_str" areadonly="false" aquery="query_topic"/>
    </SID:Attributes>
    <SID:Iterator/>
    <SID:Callbacks>
      <SID:Callback call="set_topic(topic)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
