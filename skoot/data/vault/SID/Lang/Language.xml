<object clone="/usr/SID/obj/sidnode" owner="SID">
  <SID:Element ns="Lang" tag="Language" recpoint="false" transient="false">
    <SID:Children>
      <SID:Child node="OBJ(SID:Lang:Syl)"/>
    </SID:Children>
    <SID:Attributes>
      <SID:Attribute id="language" atype="lpc_str" areadonly="false"/>
    </SID:Attributes>
    <SID:Iterator var="language" call="query_langs"/>
    <SID:Callbacks>
      <SID:Callback call="clear_langs(language)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
