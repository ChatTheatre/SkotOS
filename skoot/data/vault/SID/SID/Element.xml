<object clone="/usr/SID/obj/sidnode" owner="SID">
  <SID:Element ns="SID" tag="Element" recpoint="false" transient="false">
    <SID:Children>
      <SID:Child node="OBJ(SID:SID:Children)"/>
      <SID:Child node="OBJ(SID:SID:Attributes)"/>
      <SID:Child node="OBJ(SID:SID:Iterator)"/>
      <SID:Child node="OBJ(SID:SID:Callbacks)"/>
    </SID:Children>
    <SID:Attributes>
      <SID:Attribute id="type" atype="lpc_str" areadonly="false" aquery="query_type"/>
      <SID:Attribute id="ns" atype="lpc_str" areadonly="false" aquery="query_namespace"/>
      <SID:Attribute id="tag" atype="lpc_str" areadonly="false" aquery="query_tag"/>
      <SID:Attribute id="recpoint" atype="xml_bool" areadonly="false" aquery="query_recursion_point"/>
      <SID:Attribute id="transient" atype="xml_bool" areadonly="false" aquery="query_transient"/>
      <SID:Attribute id="query" atype="lpc_str" areadonly="false" aquery="make_query"/>
      <SID:Attribute id="newitem" atype="lpc_str" areadonly="false" aquery="make_newitem"/>
      <SID:Attribute id="delitem" atype="lpc_str" areadonly="false" aquery="make_delitem"/>
      <SID:Attribute id="comment" atype="lpc_str" areadonly="false" aquery="query_comment"/>
    </SID:Attributes>
    <SID:Iterator/>
    <SID:Callbacks>
      <SID:Callback call="set_name(ns, tag)"/>
      <SID:Callback call="set_recursion_point(recpoint)"/>
      <SID:Callback call="set_transient(transient)"/>
      <SID:Callback call="set_made_leaf(type, query)"/>
      <SID:Callback call="set_made_newitem(newitem)"/>
      <SID:Callback call="set_made_delitem(delitem)"/>
      <SID:Callback call="set_comment(comment)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
