<object clone="/usr/SID/obj/sidnode" owner="SID">
  <SID:Element ns="SID" tag="Child" recpoint="false" transient="false">
    <SID:Children/>
    <SID:Attributes>
      <SID:Attribute id="node" atype="lpc_obj" areadonly="false"/>
    </SID:Attributes>
    <SID:Iterator var="node" call="query_children"/>
    <SID:Callbacks>
      <SID:Callback call="add_child(node)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
