<object clone="/usr/SID/obj/sidnode" owner="SID">
  <SID:Element ns="SkotOS" tag="CANode" recpoint="false" transient="false">
    <SID:Children>
      <SID:Child node="OBJ(SID:Notes:Notes)"/>
    </SID:Children>
    <SID:Attributes>
      <SID:Attribute id="width" atype="lpc_int" areadonly="false" aquery="query_width"/>
      <SID:Attribute id="height" atype="lpc_int" areadonly="false" aquery="query_height"/>
      <SID:Attribute id="delay" atype="lpc_int" areadonly="false" aquery="query_delay"/>
      <SID:Attribute id="left-condition" atype="ca_cond" areadonly="false" aquery="query_left_condition"/>
      <SID:Attribute id="left-value" atype="lpc_int" areadonly="false" aquery="query_left_value"/>
      <SID:Attribute id="right-condition" atype="ca_cond" areadonly="false" aquery="query_right_condition"/>
      <SID:Attribute id="right-value" atype="lpc_int" areadonly="false" aquery="query_right_value"/>
      <SID:Attribute id="top-condition" atype="ca_cond" areadonly="false" aquery="query_top_condition"/>
      <SID:Attribute id="top-value" atype="lpc_int" areadonly="false" aquery="query_top_value"/>
      <SID:Attribute id="bottom-condition" atype="ca_cond" areadonly="false" aquery="query_bottom_condition"/>
      <SID:Attribute id="bottom-value" atype="lpc_int" areadonly="false" aquery="query_bottom_value"/>
      <SID:Attribute id="iteration-rule" atype="merry" areadonly="false" aquery="query_rule"/>
      <SID:Attribute id="state" atype="bitmap" areadonly="false" aquery="query_lattice"/>
    </SID:Attributes>
    <SID:Iterator/>
    <SID:Callbacks>
      <SID:Callback call="set_width(width)"/>
      <SID:Callback call="set_height(height)"/>
      <SID:Callback call="set_delay(delay)"/>
      <SID:Callback call="set_left_condition(left-condition)"/>
      <SID:Callback call="set_left_value(left-value)"/>
      <SID:Callback call="set_right_condition(right-condition)"/>
      <SID:Callback call="set_right_value(right-value)"/>
      <SID:Callback call="set_top_condition(top-condition)"/>
      <SID:Callback call="set_top_value(top-value)"/>
      <SID:Callback call="set_bottom_condition(bottom-condition)"/>
      <SID:Callback call="set_bottom_value(bottom-value)"/>
      <SID:Callback call="set_rule(iteration-rule)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
