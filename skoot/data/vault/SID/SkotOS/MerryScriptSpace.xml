<object clone="/usr/SID/obj/sidnode" owner="shentino">
  <SID:Element ns="SkotOS" tag="MerryScriptSpace" recpoint="false" transient="false">
    <SID:Children/>
    <SID:Attributes>
      <SID:Attribute id="space" atype="lpc_str" areadonly="false"/>
      <SID:Attribute id="handler" atype="lpc_obj" areadonly="false" aquery="query_script_space(space)"/>
    </SID:Attributes>
    <SID:Iterator var="space" call="query_script_space_indices"/>
    <SID:Callbacks>
      <SID:Callback call="register_script_space(space, handler)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
