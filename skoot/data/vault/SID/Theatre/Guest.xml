<object clone="/usr/SID/obj/sidnode" owner="SID">
  <SID:Element ns="Theatre" tag="Guest" recpoint="false" transient="false" delitem="del_guest(name)">
    <SID:Children/>
    <SID:Attributes>
      <SID:Attribute id="name" atype="lpc_str" areadonly="false"/>
    </SID:Attributes>
    <SID:Iterator var="name" call="query_guests"/>
    <SID:Callbacks>
      <SID:Callback call="add_guest(name)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
