<object clone="/usr/SID/obj/sidnode" owner="SID">
  <SID:Element ns="NpcBodies" tag="Traits" recpoint="false" transient="false" delitem="clear_trait(body, trait)">
    <SID:Children/>
    <SID:Attributes>
      <SID:Attribute id="trait" atype="lpc_str" areadonly="false"/>
    </SID:Attributes>
    <SID:Iterator var="trait" call="query_traits(body)"/>
    <SID:Callbacks>
      <SID:Callback call="add_trait(body, trait)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
