      <SID:Child node="OBJ(SID:TextIF:CombatEntry)"/>
    </SID:Children>
    <SID:Attributes>
      <SID:Attribute id="name" atype="lpc_str" areadonly="false" aquery="query_name"/>
    </SID:Attributes>
    <SID:Iterator/>
    <SID:Callbacks>
      <SID:Callback call="set_name(name)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
