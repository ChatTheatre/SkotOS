<object clone="/usr/SID/obj/sidnode" owner="SID">
  <SID:Element ns="Memories" tag="Description" recpoint="false" transient="false">
    <SID:Children/>
    <SID:Attributes>
      <SID:Attribute id="desc" atype="lpc_str" areadonly="false"/>
    </SID:Attributes>
    <SID:Iterator var="desc" call="query_desc(topic)"/>
    <SID:Callbacks>
      <SID:Callback call="add_memory(topic, desc)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
