<object clone="/usr/SID/obj/sidnode" owner="SID">
  <SID:Element ns="Marrach" tag="SkillCategory" recpoint="true" transient="false" delitem="remove_category(category)">
    <SID:Children>
      <SID:Child node="OBJ(SID:Marrach:Skills)"/>
    </SID:Children>
    <SID:Attributes>
      <SID:Attribute id="category" atype="lpc_str" areadonly="false"/>
    </SID:Attributes>
    <SID:Iterator var="category" call="list_categories"/>
    <SID:Callbacks>
      <SID:Callback call="add_category(category)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
