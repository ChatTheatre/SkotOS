<object clone="/usr/SID/obj/sidnode" owner="SID">
  <SID:Element ns="Marrach" tag="DLLExtra" recpoint="false" transient="false">
    <SID:Children/>
    <SID:Attributes>
      <SID:Attribute id="os" atype="lpc_str" areadonly="false" aquery="query_os(entry)"/>
      <SID:Attribute id="guid" atype="lpc_str" areadonly="false" aquery="query_guid(entry)"/>
    </SID:Attributes>
    <SID:Iterator/>
    <SID:Callbacks>
      <SID:Callback call="set_os(entry, os)"/>
      <SID:Callback call="set_guid(entry, guid)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
