    <SID:Children/>
    <SID:Attributes>
      <SID:Attribute id="property" atype="lpc_str" areadonly="false"/>
    </SID:Attributes>
    <SID:Iterator var="property" call="query_property_indices(#0)"/>
    <SID:Callbacks>
      <SID:Callback call="set_property(property, CONTENT)"/>
    </SID:Callbacks>
  </SID:Element>
</object>
