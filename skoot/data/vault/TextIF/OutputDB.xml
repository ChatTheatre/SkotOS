       <describe what="$(who)"/> revokes <possessive what="$(who)"/> offer of <describemany what="$(obs)" actor="$(who)" style="1"/> to <describe what="$(target)"/>.<br/>
    </TextIF:OutputThird>
    <TextIF:OutputThird id="Output:Salute">
       <describe what="$(who)"/> salutes <describemany what="$(opponent)"/>.<br/>
    </TextIF:OutputThird>
    <TextIF:OutputThird id="Output:StartExamine">
       {? | $(obs) |<describe what="$(who)" cap="1"/> begins to examine <describe what="$(obs)" actor="$(who)" self="1"/>.<br/>|<describe what="$(who)"/> begins to examine <possessive what="$(who)"/> surroundings.<br/>}
    </TextIF:OutputThird>
    <TextIF:OutputThird id="Output:StudyRequest">
       <describe what="$(student)"/> asks <describe what="$(teacher)"/> if <pronoun what="$(teacher)"/> can help <objective what="$(student)"/> improve <possessive what="$(student)"/> $(skill) skill.<br/>
    </TextIF:OutputThird>
    <TextIF:OutputThird id="Output:StudyRequestAccept">
       <describe what="$(teacher)"/> accepts <describe-poss what="$(student)"/> request and starts practicing <possessive what="$(student)"/> $(skill) skill with <objective what="$(student)"/>.<br/>
    </TextIF:OutputThird>
    <TextIF:OutputThird id="Output:StudyStop">
       <describe what="$(student)"/> stops practicing <possessive what="$(student)"/> $(skill) skill with <describe what="$(teacher)"/>.<br/>
    </TextIF:OutputThird>
    <TextIF:OutputThird id="Output:StudyStopGain">
       <describe what="$(student)"/> feels that <pronoun what="$(student)"/> is now better at $(skill) due to the teaching of <describe what="$(teacher)"/>.<br/>
    </TextIF:OutputThird>
    <TextIF:OutputThird id="Output:StudyStopGain2">
       <describe what="$(student)"/> feels that <pronoun what="$(student)"/> is now better at $(skill) due to the teaching of <describe what="$(teacher)"/>, supplemented by past lessons.<br/>
    </TextIF:OutputThird>
    <TextIF:OutputThird id="Output:StudyStopNoGain">
       <describe what="$(student)"/> learned some $(skill) skills from <describe what="$(teacher)"/> but needs more practice.<br/>
    </TextIF:OutputThird>
    <TextIF:OutputThird id="Output:Take">
       <describe what="$(who)" cap="1"/> takes <describemany what="$(obs)" actor="$(who)" style="1"/>{? | $(from) | from <describe what="$(from)" actor="$(who)"/>}.<br/>
    </TextIF:OutputThird>
    <TextIF:OutputThird id="Output:TeachOffer">
       <describe what="$(teacher)"/> offers to help <describe what="$(student)"/> improve <possessive what="$(student)"/> $(skill) skill.<br/>
    </TextIF:OutputThird>
    <TextIF:OutputThird id="Output:TeachOfferAccept">
       <describe what="$(student)"/> accepts <describe-poss what="$(teacher)"/> offer and starts practicing <possessive what="$(student)"/> $(skill) skill with <describe what="$(teacher)"/>.<br/>
    </TextIF:OutputThird>
    <TextIF:OutputThird id="Output:TeachStudyMessage">
       <describe what="$(student)"/> continues {to {receive {education on|instruction on}|learn}|learning|demonstrating|practicing} $(skill) {under the {guiding {eyes|words}|tutelage} of|from} {<possessive what="$(student)"/> teacher|<describe what="$(teacher)"/>}.<br/>
    </TextIF:OutputThird>
    <TextIF:OutputThird id="Output:Touch">
       {? equal | $(obs) | $(who) |<describe what="$(who)"/> touches <objective what="$(who)"/>self.<br/>|<describe what="$(who)"/> touches <describe what="$(obs)"/>.<br/>}
    </TextIF:OutputThird>
    <TextIF:OutputThird id="Output:Undrag">
       <describe what="$(dragger)"/> releases <describemany what="$(victim)" actor="$(dragger)" style="1"/>.<br/>
    </TextIF:OutputThird>
    <TextIF:OutputThird id="Output:Unwear">
       <describe what="$(who)" cap="1"/> removes <describemany what="$(obs)" actor="$(who)" style="1"/>.<br/>
    </TextIF:OutputThird>
    <TextIF:OutputThird id="Output:Unwield">
       <describe what="$(who)" cap="1"/> unwields <describemany what="$(obs)" actor="$(who)" style="1"/>.<br/>
    </TextIF:OutputThird>
    <TextIF:OutputThird id="Output:Wear">
       {? | $(clothing) |<describe what="$(who)" cap="1"/> wears <describemany what="$(clothing)" actor="$(who)" style="1"/>.<br/>}{? | $(weapons) |<describe what="$(who)" cap="1"/> wields <describemany what="$(weapons)" actor="$(who)" style="1"/>.<br/>}
    </TextIF:OutputThird>
  </TextIF:OutputDB>
</object>
