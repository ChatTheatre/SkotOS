/*add superbrief to names if not book*/
if(this."trait:superbrief" != "book")\{
 Set(this, "details:default:sname:" + this."trait:superbrief", this."trait:superbrief" + "s");
\}

/*how many pages will this book have?*/
pgcount=16;
if(this."trait:pagecount"!=nil)\{
 pgcount=this."trait:pagecount";
\}

/*make all the pages*/
for(ctr=0;ctr\<pgcount;ctr++)\{
 mgconstruction::initializeconstructeddetails(\$ob1: this, \$objname: "MGeneric:UrPage", \$detail: "default");
\}

/*copy content to pages*/


